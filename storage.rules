rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /marketplace/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
      allow delete: if request.auth != null && (request.auth.token.email == 'admin' || request.auth.uid == 'admin');
    }
  }
}
